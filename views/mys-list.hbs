<h2>Mysterious Places</h2>

<div class="row">
  {{#each mysArray}}
  <div class="column">
    <div class="content">
      <a class="img-center" href="/mysteriousplaces/{{this.id}}"><img class="img-list" src={{this.images.[0]}} alt="mysterious-image"></a>

      <h3>{{this.placeName}}</h3>
    </div>
  </div>
  {{/each}}
</div>