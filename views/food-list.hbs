<h2>Food</h2>

<div class="row">
    {{#each foodArray}}
    <div class="column">
        <div class="content">
            <a class="img-center" href="/food/{{this.id}}"><img class="img-list" src={{this.images.[0]}} alt="food-image"></a>
            <h3>{{this.foodName}}</h3>
        </div>
    </div>
    {{/each}}
</div>