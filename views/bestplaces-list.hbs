<h2>Best Places </h2>

<div class="row">
    {{#each bestPlacesArray}}

    <div class="column">
        <div class="content">

            <a class="img-center" href="/bestplaces/{{this.id}}">
                <img class="img-list" src={{this.images.[0]}} alt="bestplaces-image">
            </a>
            <h3>{{this.placeName}}</h3>

        </div>
    </div>

    {{/each}}
</div>